var message1 = errors.length > 0
  ? errors.join("\n")
  : "All datatypes and constraints are valid";

if (message1 === "All datatypes and constraints are valid") {

  // Always new ID
  const dummyId = cds.utils.uuid();

  // Insert JSON into dummy table
  await tx.run(
    INSERT.into("COM_SCB_FILEUPLOAD_MASTER_DUMMY").entries({
      ID: dummyId,
      JSON_DATA: JSON.stringify(jsonData)
    })
  );

  // Call duplicate validator procedure
  await tx.run(
    `CALL "GENERIC_DATA_VALIDATOR"(?, ?, ?, ?)`,
    [
      dummyId,
      MODULE_NAME,
      SUB_MODULE_NAME,
      LOAD_TEMPLATE
    ]
  );

  // Read latest validation result
  const result = await tx.run(
    SELECT.one
      .from("COM_SCB_FILEUPLOAD_MASTER_DUPLICATEVALIDATIONRESULT")
      .where({ ID: dummyId })
      .orderBy({ CREATED_AT: "desc" })
  );

  if (!result) {
    throw new Error("Duplicate validation result not found");
  }

  const status = result.STATUS;   // ✅ CORRECT
  const message = result.MESSAGE; // ✅ CORRECT

  // Cleanup dummy data
  await tx.run(
    DELETE.from("COM_SCB_FILEUPLOAD_MASTER_DUMMY")
      .where({ ID: dummyId })
  );

  // Handle duplicate error
  if (status === "E") {
    return {
      flag: false,
      message: message
    };
  }

  return {
    flag: true,
    message: "Proceed"
  };

} else {
  return {
    flag: false,
    message: message1
  };
}
