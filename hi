var message1 = errors.length > 0
  ? errors.join("\n")
  : "All datatypes and constraints are valid";

if (message1 === "All datatypes and constraints are valid") {

  // ðŸ”‘ Always new ID per upload
  const dummyId = cds.utils.uuid();

  // ðŸ”‘ Clean only THIS upload's old data (safety)
  await tx.run(
    DELETE.from("COM_SCB_FILEUPLOAD_MASTER_DUMMY")
      .where({ ID: dummyId })
  );

  await tx.run(
    DELETE.from("COM_SCB_FILEUPLOAD_MASTER_DUPLICATEVALIDATIONRESULT")
      .where({ ID: dummyId })
  );

  // Insert JSON
  await tx.run(
    INSERT.into("COM_SCB_FILEUPLOAD_MASTER_DUMMY").entries({
      ID: dummyId,
      JSON_DATA: JSON.stringify(jsonData)
    })
  );

  // Call procedure
  await tx.run(
    `CALL "GENERIC_DATA_VALIDATOR"(?, ?, ?, ?)`,
    [
      dummyId,
      MODULE_NAME,
      SUB_MODULE_NAME,
      LOAD_TEMPLATE
    ]
  );

  // Read result
  const result = await tx.run(
    SELECT.one
      .from("COM_SCB_FILEUPLOAD_MASTER_DUPLICATEVALIDATIONRESULT")
      .where({ ID: dummyId })
  );

  // Cleanup dummy AFTER read
  await tx.run(
    DELETE.from("COM_SCB_FILEUPLOAD_MASTER_DUMMY")
      .where({ ID: dummyId })
  );

  if (!result) {
    throw new Error("Duplicate validation result not found");
  }

  if (result.STATUS === "E") {
    return {
      flag: false,
      message: result.MESSAGE
    };
  }

  return {
    flag: true,
    message: "Proceed"
  };

} else {
  return {
    flag: false,
    message: message1
  };
}
